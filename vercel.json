{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "headers": [
    {
      "source": "/api/(.*.php)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=UTF-8"
        }
      ]
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}