{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/ga6/?$",
      "dest": "/group_assignments/ga6/zip.html"
    },
    {
      "src": "/ga6/(.*)",
      "dest": "/group_assignments/ga6/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
